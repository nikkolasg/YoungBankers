<%= if @users.blank? %>
<p>There are not any users currently in the system.</p>
<% else %>
    <p>These are the current users in our system</p>
    <ul id="users">
        <% @users.each do |c| %>
            <li> <%= puts c.fname + " " + c.lname + " " + c.email %> </li>
            <li><%= link_to "User Profile", user_path, :id => c.id} %></li>
        <% end %>
    </ul>
<% end %>
