<% if @users.blank? %>
<p>There are not any users currently in the system.</p>
<% else %>
    
    <%= will_paginate %>
    <p>These are the current users in our system</p>
    <ul class="users">
        <% @users.each do |user| %>
            <%= render user %>
        <%  end %>
    </ul>
    <%= will_paginate %>
<% end %>
<p> <%= link_to "Sign in", signin_path %> 
<p> <%= link_to "Sign out", signout_path, method: "delete" %> </p>
